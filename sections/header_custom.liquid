{% schema %}
  {
    "name": "Header custom",
    "settings": [
      {
        "type": "image_picker",
        "id": "logo",
        "label": "Selecione a logo"
      },
      {
        "type": "link_list",
        "id": "menu",
        "label": "Selecione o menu"
      },
      {
        "type": "image_picker",
        "id": "image_1",
        "label": "Image"
      },
      {
        "type": "text",
        "id": "title_collection",
        "label": "Heading"
      }, {
        "type": "text",
        "id": "image_1_text",
        "label": "Text"
      }, {
        "type": "url",
        "id": "image_1_link",
        "label": "Link"
      }
    ],
    "presets": [
      {
        "name": "Header custom",
        "category": "header"
      }
    ]
  }
{% endschema %}

